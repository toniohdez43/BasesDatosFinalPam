@model IEnumerable<ProyectoFinalPam.Models.ConsultaAlumno_Result>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <input type="text" id="alumno" class="form-control" />
    <input type="submit" />
}

@foreach (var p in Model)
{
    <strong>@p.Nombre</strong>@p.ApellidoP<strong>@p.ApellidoM</strong><br />
}
